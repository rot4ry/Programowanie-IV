<Window x:Class="Lab11._2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab11._2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="MyTextBox" HorizontalAlignment="Left" Height="264" Margin="32,26,0,0" TextWrapping="Wrap" Text="{Binding Content}" VerticalAlignment="Top" Width="707"/>
        <Button Content="fileDialog" HorizontalAlignment="Left" Margin="32,321,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
